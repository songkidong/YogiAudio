<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project3.yogiaudio.filedb.repository.FiledbRepository">

	<insert id="saveFiledb">
		insert into filedb
		values(#{uuid}, #{originalFileName}, #{fileData});
	</insert>

	<select id="findByUuid"
		resultType="com.project3.yogiaudio.filedb.entity.Filedb">
		select * from filedb where uuid = #{uuid}
	</select>


</mapper>